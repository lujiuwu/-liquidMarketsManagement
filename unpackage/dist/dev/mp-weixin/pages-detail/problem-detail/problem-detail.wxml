<view class="problem-detail"><view class="bg-image"><image class="problem_image" src="{{problem.problem_image}}"></image><view class="problem_title">{{''+problem.problem_title+''}}</view></view><view class="detail_des"><view class="show_tabs"><u-tabs vue-id="3bdafbbe-1" list="{{list}}" overlay="false" is-scroll="{{false}}" current="{{current}}" itemStyle="width:50%;padding:20rpx 0;color:#fff;" activeStyle="{{({color:'#000',fontSize:'32rpx',fontWeight:'bold'})}}" lineWidth="0" inactiveStyle="{{({color:'#242424',fontSize:'32rpx'})}}" scrollable="{{false}}" data-event-opts="{{[['^click',[['Detailchange']]]]}}" bind:click="__e" bind:__l="__l"></u-tabs></view><view hidden="{{!(current==0)}}" class="detail_massage"><view class="problem_submit single_message"><text class="message_name">提交人员：</text><my-submit vue-id="3bdafbbe-2" submit_info="{{submit_info}}" bind:__l="__l"></my-submit></view><view class="problem_shop single_message"><text class="message_name">店铺名称：</text><text class="message_content">{{problem.problem_shop}}</text></view><view class="probelem_time single_message"><text class="message_name">提交时间：</text><text class="message_content">{{problem.problem_time}}</text></view><view class="problem_code single_message"><text class="message_name">问题编号：</text><text class="message_content">{{problem.problem_code}}</text></view><view class="probelem_status single_message"><text class="message_name">问题状态：</text><text class="message_content">{{problem.problem_status}}</text></view><view class="probelem_tags single_message"><text class="message_name">问题内容：</text><block wx:for="{{problem.problem_tags}}" wx:for-item="tag" wx:for-index="index" wx:key="index"><view class="single-tag">{{''+tag+''}}</view></block></view><view class="probelem_proof single_message"><text class="message_name">佐证材料：</text><view hidden="{{!(problem.problem_proof)}}" class="proof"><block wx:for="{{problem.problem_proof}}" wx:for-item="proof" wx:for-index="index"><view class="single_proof"><image class="proof_image" src="{{proof}}" data-event-opts="{{[['tap',[['previewPhoto',[index]]]]]}}" bindtap="__e"></image></view></block></view><text hidden="{{!(!problem.problem_proof)}}">暂无</text></view></view><view hidden="{{!(current==1)}}" class="map"><map class="page-section" style="{{'height:'+('1000rpx')+';'}}" layer-style="{{5}}" show-location="{{true}}" latitude="{{map_info.latitude}}" longitude="{{map_info.longitude}}" markers="{{map_info.marker}}" scale="{{scale}}" data-event-opts="{{[['markertap',[['markertap',['$event']]]],['callouttap',[['callouttap',['$event']]]]]}}" bindmarkertap="__e" bindcallouttap="__e"></map></view></view><view class="solve-problem"><my-solve-problem vue-id="3bdafbbe-3" status="{{problem.problem_status}}" bind:__l="__l"></my-solve-problem></view></view>